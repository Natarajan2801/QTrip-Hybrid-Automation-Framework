<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="QTrip UI Suite" parallel="methods" thread-count="3" verbose="2">

    <listeners>
        <listener class-name="com.qtrip.listeners.TestListener"/>
        <listener class-name="com.qtrip.listeners.AnnotationTransformer"/>
    </listeners>

    <test name="Functional UI Tests">
        <classes>
            <class name="com.qtrip.tests.ui.AuthenticationTest"/>
            <class name="com.qtrip.tests.ui.SearchTest"/>
            <class name="com.qtrip.tests.ui.BookingFlowTest"/>
            <class name="com.qtrip.tests.ui.ReliabilityTest"/>
        </classes>
    </test>
</suite>